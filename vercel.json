{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "pair.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/qr", "dest": "index.js" },
    { "src": "/code", "dest": "index.js" },
    { "src": "/pair", "dest": "pair.js" },
    { "src": "/", "dest": "index.js" }
  ]
}
